config_version=4

[application]

config/name="KINDERGARTEN MASSAKER"
run/main_scene="res://SCENES/TMP/The_Game.tscn"
boot_splash/image="res://ASSETS/GRAPHICS/MISC/Splash_Screen.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://KM_LOGO.png"
config/windows_native_icon="res://KM_LOGO.ico"

[autoload]

Autoload="*res://SCRIPTS/TMP/Autoload.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/width=480
window/size/height=270
window/size/always_on_top=true
window/size/test_width=960
window/size/test_height=540
mouse_cursor/tooltip_position_offset=Vector2( 10, 20 )
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[global]

second=false
fps=false
frame=false
wind=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
